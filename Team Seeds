// C++ code
//
#include <Servo.h>;
Servo Frontwheels;
Servo Quadrant;
Servo PipeHoleMaker;
void setup()
{
  pinMode(13, OUTPUT);
  pinMode(12,OUTPUT);
  pinMode(11,OUTPUT);
  pinMode(7,OUTPUT);
  pinMode(6,OUTPUT);
  Frontwheels.attach (13);
  Quadrant.attach (12);
  PipeHoleMaker.attach (11);
}

void loop()
{
  digitalWrite(LED_BUILTIN, HIGH);
  delay(1000); // Wait for 1000 millisecond(s)
  digitalWrite(LED_BUILTIN, LOW);
  delay(1000); // Wait for 1000 millisecond(s)
}
