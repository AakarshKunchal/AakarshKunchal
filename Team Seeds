// C++ code
//
#include <Servo.h>;
Servo Frontwheels;
Servo Quadrant;
Servo PipeHoleMaker;
void setup()
{
  pinMode(13, OUTPUT);
  pinMode(12,OUTPUT);
  pinMode(11,OUTPUT);
  pinMode(3,OUTPUT);
  pinMode(4,OUTPUT);
  Frontwheels.attach (13);
  Quadrant.attach (12);
  PipeHoleMaker.attach (11);
}

void loop()
{
digitalWrite (4,HIGH);
delay ()
}
